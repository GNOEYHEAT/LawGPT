<ROW><CELL ColAddr="0" RowAddr="38" ColSpan="1" RowSpan="1" Width="16819" Height="282" Header="false" HasMargin="false" Protect="false" Editable="false" Dirty="false" BorderFill="19"><PARALIST TextDirection="0" LineWrap="Break" VertAlign="Top"><P ParaShape="2"><TEXT CharShape="6"/></P>
<P ParaShape="22"><TEXT CharShape="76"><CHAR>     제2편 회생절차</CHAR></TEXT><TEXT CharShape="75"><CHAR>&nbsp;</CHAR></TEXT></P>
</PARALIST></CELL>
<CELL ColAddr="1" RowAddr="38" ColSpan="1" RowSpan="1" Width="16819" Height="282" Header="false" HasMargin="false" Protect="false" Editable="false" Dirty="false" BorderFill="19"><PARALIST TextDirection="0" LineWrap="Break" VertAlign="Top"><P ParaShape="22"><TEXT CharShape="6"><CHAR></CHAR></TEXT></P></PARALIST></CELL>
